<o:TriggerItem Id="o267">
<a:ObjectID>B372665E-1820-47CA-9924-B9791D8327AD</a:ObjectID>
<a:Name>UpdateAfterLog</a:Name>
<a:Code>UpdateAfterLog</a:Code>
<a:Text>.IF (%TABLE% == &quot;ASTLOGER&quot;) || (%TABLE% == &quot;ASTLOGTA&quot;)|| (%TABLE% == &quot;ASTLOGPA&quot;)
    
    /* É uma tabela de LOG, por isso não vai ser gerado uma Trigger igual as outras tabelas */
    
.ELSE

    /* Checa se o arquivo eh pra salvar o log de alteracao do registro */
    SELECT LOG_ALTER, SAVE_ID_DELETE 
        FROM ASTARCHI 
        WHERE ASTARCHI.FILE =  &#39;%TABLE%&#39; INTO :LOG_UPD, :SAVE_ID_DEL;

    /* Verifica se nao precisa gerar o log de update dessa tabela. */
    IF (IIF(:LOG_UPD IS NULL, &#39;&#39;, :LOG_UPD) &lt;&gt; &#39;0&#39;) THEN BEGIN
        DESC_LOG = &#39;{&#39;; -- Inicia a variavel que vai salvar a descricao do log no formato JSON
    
        .DEFINE &quot;NOT_FIRST&quot; &quot;false&quot; .// Variavel para verificar se eh a primeira coluna
        .DEFINE &quot;EXIST_ASTEMPRE&quot; &quot;false&quot; .// Variavel para ver se essa tabela tem a coluna ID_ASTEMPRE
        .FOREACH_COLUMN()
            .// Verifica o tipo de dados da coluna, se é do tipo INTERGER
            .DEFINE &quot;IS_INTEGER&quot; &quot;[%COLTYPE%=INTEGER? true : false]&quot;
            .DEFINE &quot;IS_BIGINT&quot; &quot;[%COLTYPE%=BIGINT? true : false]&quot;
            .DEFINE &quot;IS_ID_ASTEMPRE&quot; &quot;[%COLNNAME%=ID_ASTEMPRE? true : false]&quot; .// Variavel para ver se essa tabela tem a coluna ID_ASTEMPRE
            .// Checa se eh a primeiro coluna para adicionar uma virgula pra ficar no formato JSON
            .// Checa se a coluna eh do tipo INTEGER
            .IF (%IS_INTEGER%) || (%IS_BIGINT%)
            
                .//IF (((OLD.%COLUMN% Is Null) And (NEW.%COLUMN% Is Not Null)) Or ((OLD.%COLUMN% Is Not Null) And (NEW.%COLUMN% Is Null)) Or (OLD.%COLUMN% &lt;&gt; NEW.%COLUMN%)) THEN DESC_LOG = [%NOT_FIRST%? &#39;, &#39; || :] :DESC_LOG || &#39;&quot;%COLUMN%&quot;: &#39; || IIf(OLD.%COLUMN% Is Null, &#39;&#39;, OLD.%COLUMN% || &#39;,&#39;);
                IF (((OLD.%COLUMN% Is Null) And (NEW.%COLUMN% Is Not Null)) Or ((OLD.%COLUMN% Is Not Null) And (NEW.%COLUMN% Is Null)) Or (OLD.%COLUMN% &lt;&gt; NEW.%COLUMN%)) THEN DESC_LOG = :DESC_LOG || &#39;&quot;%COLUMN%&quot;: &#39; || IIf(OLD.%COLUMN% Is Null, &#39;NULL,&#39;, OLD.%COLUMN% || &#39;,&#39;);
            .ELSE
        
                .//IF (((OLD.%COLUMN% Is Null) And (NEW.%COLUMN% Is Not Null)) Or ((OLD.%COLUMN% Is Not Null) And (NEW.%COLUMN% Is Null)) Or (OLD.%COLUMN% &lt;&gt; NEW.%COLUMN%)) THEN DESC_LOG = [%NOT_FIRST%? &#39;, &#39; || :] :DESC_LOG || &#39;&quot;%COLUMN%&quot;: &#39; || IIf(OLD.%COLUMN% Is Null, &#39;&quot;&quot;&#39;, &#39;&quot;&#39; || OLD.%COLUMN% || &#39;&quot;&#39;);
                IF (((OLD.%COLUMN% Is Null) And (NEW.%COLUMN% Is Not Null)) Or ((OLD.%COLUMN% Is Not Null) And (NEW.%COLUMN% Is Null)) Or (OLD.%COLUMN% &lt;&gt; NEW.%COLUMN%)) THEN DESC_LOG = :DESC_LOG || &#39;&quot;%COLUMN%&quot;: &#39; || IIf(OLD.%COLUMN% Is Null, &#39;&quot;&quot;,&#39;, &#39;&quot;&#39; || OLD.%COLUMN% || &#39;&quot;,&#39;);
            .ENDIF
            .// Marca a variavel dizendo que nao eh mais a primeira coluna
            .DEFINE &quot;NOT_FIRST&quot; &quot;true&quot;
            .// Checa se é a coluna ID_astempre
            .IF (%IS_ID_ASTEMPRE%)
                .DEFINE &quot;EXIST_ASTEMPRE&quot; &quot;true&quot;
            .ENDIF
        .ENDFOR
    
        DESC_LOG = :DESC_LOG || &#39;}&#39;; -- Finaliza o JSON
    
        .// Gera o log da tabela em uma banco separado
        .// Foi comentado para não inserir o log em outro banco devido o desempenho e devido nem toda a transação ter sucesso
        .// Com isso foi retirado a execução da procedure de log que inseri em outro banco de dados
        .//EXECUTE PROCEDURE PROC_LOG_INSERT(:ID_ASTACCOU_LOGIN, [%EXIST_ASTEMPRE%? OLD.ID_ASTEMPRE : NULL], &#39;U&#39;, &#39;%TABLE%&#39;, OLD.ID_%TABLE%, &#39;NOW&#39;, :DESC_LOG, NULL) RETURNING_VALUES :ID_ASTLOGTA;
        .//IF ( :ID_ASTLOGTA &lt;= 0 ) THEN
        INSERT INTO ASTLOGER (ID_ASTCONTA_CADASTRO, ID_ASTEMPRE, TIPO_OPERACAO, ARQUIVO, ID_REGISTRO, DT_OPERACAO, DESCRICAO_LOG)
            VALUES ( IIF(:ID_ASTACCOU_LOGIN &lt;= 0, NULL, :ID_ASTACCOU_LOGIN), [%EXIST_ASTEMPRE%? OLD.ID_ASTEMPRE : NULL], &#39;U&#39;, &#39;%TABLE%&#39;, OLD.ID_%TABLE%, &#39;NOW&#39;, :DESC_LOG);

    END
.ENDIF</a:Text>
