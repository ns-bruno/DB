<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, VIEW}

select distinct
   t.rdb$owner_name,
   v.rdb$view_name
from
   rdb$view_relations v,
   rdb$relations t
where
   v.rdb$view_name = t.rdb$relation_name
[  and t.rdb$owner_name=%.q:SCHEMA%]
order by
   1, 2
</a:TextTargetItem.Value>
