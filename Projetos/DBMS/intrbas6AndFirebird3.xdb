<o:TriggerItem Id="o261">
<a:ObjectID>A18EBDDD-4D39-469E-8E33-9447364CA4A2</a:ObjectID>
<a:Name>DeleteAlways</a:Name>
<a:Code>DeleteAlways</a:Code>
<a:Text>/* Checa se o arquivo eh pra salvar o id deletado */
SELECT LOG_DELETE, SAVE_ID_DELETE 
    FROM ASTARCHI 
    WHERE ASTARCHI.FILE =  &#39;%TABLE%&#39; INTO :LOG_DEL, :SAVE_ID_DEL;
/* Se essa tabela esta marcada para salvar o id delacao então salva o id em uma tabela a parte para replicar o arquivo deletados em outros banco de dados que estao replicados bases entre filiais. */    
IF (IIF(:SAVE_ID_DEL IS NULL, &#39;&#39;, :SAVE_ID_DEL) &lt;&gt; &#39;0&#39;) THEN INSERT INTO ASTREGDE (FILE_ARCHI, ID_DELETE, UUID_DELETE) VALUES (&#39;%TABLE%&#39;, OLD.ID_%TABLE%, OLD.UUID);

/* Verifica se pode gerar o log de delete dessa tabela. */
IF (:LOG_DEL &lt;&gt; &#39;0&#39;) THEN BEGIN
    DESC_LOG = &#39;{&#39;; -- Inicia a variavem que vai salvar a descricao do log no formato JSON
    
    .DEFINE &quot;NOT_FIRST&quot; &quot;false&quot; .// Variavel para verificar se eh a primeira coluna
    .FOREACH_COLUMN()
        .// Verifica o tipo de dados da coluna, se é do tipo INTERGER
        .DEFINE &quot;IS_INTEGER&quot; &quot;[%COLTYPE%=INTEGER ? true : false]&quot;
        .// Checa se eh a primeiro coluna para adicionar uma virgula pra ficar no formato JSON
        .// Checa se a coluna eh do tipo INTEGER
        .IF (%IS_INTEGER%)
            
            IF (IIF(OLD.%COLUMN% Is Null, 0, OLD.%COLUMN%) &lt;&gt; 0) THEN DESC_LOG = [%NOT_FIRST%? &#39;, &#39; || :] :DESC_LOG || &#39;&quot;%COLUMN%&quot;: &#39; || OLD.%COLUMN%;
        .ELSE
            IF (OLD.%COLUMN% Is Not Null) THEN DESC_LOG = [%NOT_FIRST%? &#39;, &#39; || :] :DESC_LOG || &#39;, &quot;%COLUMN%&quot;: &#39; || &#39;&quot;&#39; || OLD.%COLUMN% || &#39;&quot;&#39;;
        .ENDIF
        .// Marca a variavel dizendo que nao eh mais a primeira coluna
        .DEFINE &quot;NOT_FIRST&quot; &quot;true&quot;
    .ENDFOR
    
    DESC_LOG = :DESC_LOG || &#39;}&#39; -- Finaliza o JSON
    
    .//IF (:DESC_LOG &lt;&gt; &#39;&#39;) THEN INSERT INTO SMALOGMV (arquivo, id_arquivo, USUARIO, dt_ult_alt_arquivo, dt_alt, tipo, log) 
    .//            values (&#39;AEAITSAI&#39;, OLD.ID_AEAITSAI, USER, OLD.dt_alt, &#39;NOW&#39;, &#39;D&#39;, :linha);
END</a:Text>
<a:CreationDate>1571930020</a:CreationDate>
<a:Creator>Nogueira</a:Creator>
<a:ModificationDate>1606078097</a:ModificationDate>
<a:Modifier>Nogueira</a:Modifier>
<a:Comment>Não é mais usado</a:Comment>
</o:TriggerItem>
