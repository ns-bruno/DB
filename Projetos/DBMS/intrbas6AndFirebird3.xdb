<o:TriggerItem Id="o245">
<a:ObjectID>22FD5692-661C-11D3-B561-00105A1AA418</a:ObjectID>
<a:Name>InsertChildParentExist</a:Name>
<a:Code>InsertChildParentExist</a:Code>
<a:Text>.FOREACH_PARENT(&quot;FKNOTNULL&quot;)
/*  Na tebela &quot;%PARENT%&quot; deve existir o registro ao inserir um um registro da ntabela &quot;%CHILD%&quot;  */
if (.JOIN(&quot;new.%FK% is not null&quot;, &quot;&quot;, &quot; and&quot;, &quot;) then&quot;)
begin
    select count(*)
        from %PARENT%
        where .JOIN(&quot;%PARENT%.%PK% = new.%FK%&quot;, &quot;and   &quot;) 
        into   :numrows;
   if (numrows = 0) then
   begin
      exception ERRINS_PARENT_NOT_EXIST (&#39; Não existe o registro &#39; || NEW.%FK% || &#39; na tabela %PARENT% .&#39;);
   end
end
.ENDFOR</a:Text>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1606263686</a:ModificationDate>
<a:Modifier>Nogueira</a:Modifier>
<a:Comment>Verifica se a tabela pai tem o registro cadastrado. Parent must exist when inserting child constraint</a:Comment>
<a:Declaration>.FOREACH_PARENT()
.ENDFOR</a:Declaration>
</o:TriggerItem>
<o:TriggerItem Id="o246">
<a:ObjectID>22FD5693-661C-11D3-B561-00105A1AA418</a:ObjectID>
<a:Name>InsertTooManyChildren</a:Name>
<a:Code>InsertTooManyChildren</a:Code>
<a:Text>.FOREACH_PARENT(&quot;FKMAXCARD&quot;)
/*  The cardinality of Parent &quot;[%PQUALIFIER%]%PARENT%&quot; in child &quot;[%CQUALIFIER%]%CHILD%&quot; cannot exceed %MAXCARD% */
if (.JOIN(&quot;new.%FK% is not null&quot;, &quot;&quot;, &quot; and&quot;, &quot;) then&quot;)
begin
   select count(*)
   from  [%CQUALIFIER%]%CHILD% T
   where  .JOIN(&quot;T.%FK% = new.%FK%&quot;, &quot;and   &quot;, &quot;&quot;, &quot;&quot;)
   into  :maxcard;

   if (maxcard &gt;= %MAXCARD%) then
      exception sdp_itmc;
   end
end
.ENDFOR</a:Text>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1567808607</a:ModificationDate>
<a:Modifier>Nogueira</a:Modifier>
<a:Comment>NÂO - Cannot exceed the maximum cardinality constraint when inserting the child</a:Comment>
<a:Declaration>declare variable maxcard integer;
.FOREACH_PARENT(&quot;FKMAXCARD&quot;)
.ENDFOR</a:Declaration>
</o:TriggerItem>
<o:TriggerItem Id="o247">
<a:ObjectID>22FD5695-661C-11D3-B561-00105A1AA418</a:ObjectID>
<a:Name>UpdateChangeColumn</a:Name>
<a:Code>UpdateChangeColumn</a:Code>
<a:Text>.FOREACH_COLUMN(&quot;NMFCOL&quot;)
/*  Non modifiable column &quot;%COLUMN%&quot; cannot be modified  */
if (old.%COLUMN% != new.%COLUMN%) then
begin
   exception ERRUPD_CANNOT_MODIFY_CLOMUN (&#39;Coluna/Campo %COLUMN%. Valor antigo &#39; || OLD.%COLUMN% ||&#39; e novo valor &#39; || NEW.%COLUMN% || &#39;.&#39;);
end
.ENDFOR</a:Text>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1595651614</a:ModificationDate>
<a:Modifier>Nogueira</a:Modifier>
<a:Comment>Caso alguma coluna/campo seja marcado para não ser modificado vai aparecer esse exception. Cannot modify non modifiable column constraint</a:Comment>
<a:Declaration>.FOREACH_COLUMN(&quot;NMFCOL&quot;)
.ENDFOR</a:Declaration>
</o:TriggerItem>
<o:TriggerItem Id="o248">
